<h2>会員情報編集</h2>
<%= form_with model: @user, url: customers_information_path do |f| %>
<label>氏名</label>
<%= f.text_field :last_name %>
<%= f.text_field :first_name %><br />
<label>フリガナ</label>
<%= f.text_field :last_name_kana %>
<%= f.text_field :first_name_kana %><br />
<label>郵便番号</label>
<%= f.telephone_field :postal_code %><br />
<label>住所</label>
<%= f.text_field :address %><br />
<label>電話番号</label>
<%= f.telephone_field :telephone_number %><br />
<label>メールアドレス</label>
<%= f.text_field :email %><br />
<%= f.submit "編集内容を保存" %>
<%= link_to "退会する", customers_confirm_withdraw_path %>
<% end %>